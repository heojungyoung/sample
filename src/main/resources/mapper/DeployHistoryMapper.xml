<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sample.sample.dao.sampleDao">
               
	<select id="selectSampleList" parameterType="String" resultType="com.sample.sample.model.sampleModel">
	    SELECT 
			ID
		    ,NAME
		    ,CNTRCODE
			,POPULATION
		FROM CITY
    </select>
   	
   	<select id="selectSvcList" parameterType="String" resultType="com.sample.sample.model.svcModel">
	    
	    SELECT    
	      VA_SVC_ID as vaSvcId ,
		  SPRT_YEAR as sprtYear,
		  VA_TYPE_CD vaTypeCd ,
		  VA_SVC_NM vaSvcNm ,
		  APP_PKG_ID appPkgId , 
		  REG_PGM_ID regPgmId ,
		  MDF_PGM_ID mdfPgmId, 
		  REGR_ID regrId, 
		  REG_DT regDt ,
		  MDFR_ID mdfrId ,
		  MDF_DT mdfDt
	    FROM TMVA_VA_SVC_M
	    
	    
    </select>
   	
   	
   	
   	
   	
</mapper>